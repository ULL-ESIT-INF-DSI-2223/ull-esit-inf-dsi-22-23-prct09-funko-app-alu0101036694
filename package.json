{
  "name": "practica09",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc src/code/app.ts",
    "express": "tsc src/express/server.ts && node dist/express/server.js",
    "server": "tsc src/client-server/server.ts",
    "client": "tsc src/client-server/client.ts",
    "add": "node src/code/app.js add --user \"carla\" --id 1 --name \"Classic Sonic\" --desc \"The best Sonic Funko ever\" --type \"Pop!\" --genre \"Games\" --franchise \"A\" --number 1 --exclusive 0 --special \"C\" --value 10",
    "update": "node src/code/app.js update --user \"carla\" --id 1 --name \"Classic Sonic 2\" --desc \"The best Sonic Funko ever\" --type \"Pop!\" --genre \"Games\" --franchise \"A\" --number 1 --exclusive 0 --special \"C\" --value 20",
    "read": "node src/code/app.js read --user \"carla\" --id 1",
    "list": "node src/code/app.js list --user \"carla\"",
    "remove": "node src/code/app.js remove --user \"carla\" --id 1",
    "start": "tsc-watch --onSuccess \"node dist/app.js\"",
    "doc": "typedoc",
    "test": "mocha -r ts-node/register 'tests/**/*.ts'",
    "mod1": "mocha -r ts-node/register 'tests/modificacion01.spec.ts'",
    "mod2": "mocha -r ts-node/register 'tests/modificacion02.spec.ts'",
    "coverage": "c8 npm test && c8 report --reporter=lcov"
  },
